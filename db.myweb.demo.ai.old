;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	robot1.docker.demo.ai. admin.docker.demo.ai. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
docker.demo.ai.    IN      NS      robot1.docker.demo.ai.
; name servers - A records
@				IN	A	192.168.64.5
robot1				IN	A	192.168.64.5
www				IN	A	192.168.64.5
swarmpit.docker.demo.ai.	IN	A	192.168.64.4
robot2.docker.demo.ai.          IN      A       192.168.64.6
robot3.docker.demo.ai.          IN      A       192.168.64.7

robot0				IN	A	192.168.64.4
robot1				IN	A	192.168.64.5
robot2				IN	A	192.168.64.6
robot3				IN	A	192.168.64.7
robot4				IN	A	192.168.64.8

docker.demo.ai			IN	CNAME	192.168.64.5
www				IN	A	192.168.64.5
jenkins				IN	A	192.168.64.5
swarmpit			IN	A	192.168.64.4
