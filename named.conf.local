// Do any local configuration here
zone "myweb.demo.ai" {
    type master;
    file "/etc/bind/zones/db.myweb.demo.ai"; # zone file path
    forwarders {};
    allow-update { none; };
};

zone "232.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/zones/db.192.168.232";  # 192.168.232.0/24 subnet
    allow-update { none; };
    forwarders {};
};
